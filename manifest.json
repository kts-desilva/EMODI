{
  "manifest_version": 2,
  "name": "Example Extension",
  "version": "1.0",
  "description": "This extension shows the pieces of a chrome extension",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "151918638096093397.png",
    "default_popup":"ipopup.html"
  },
  "background": {
    "scripts": ["background.js","jquery.min.js"]
  },
  "permissions":
   ["activeTab","tabs","alarms","storage", "notifications","http://localhost/EMODI/EMODI/login.html"]
,
   "web_accessible_resources": ["lg.png","http://localhost/EMODI/EMODI/login.html"]
  
}